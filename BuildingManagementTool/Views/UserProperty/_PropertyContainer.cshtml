@model IEnumerable<PropertyViewModel>

    @if (Model.Any())
    {
        @foreach (var userProperty in Model)
        {
            <partial name="_PropertyCard" model="userProperty"></partial>
        }
    }
    else
    {
        <h3 class="col-lg-12 text-center mt-5">
            There are no properties linked to this account. <br>
            Please add a property to manage
        </h3>
    }
<div class="errorContainer"></div>
    

